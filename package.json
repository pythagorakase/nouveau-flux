{
  "name": "nouveau-flux",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/nouveau-flux.umd.cjs",
  "module": "./dist/nouveau-flux.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/nouveau-flux.js",
      "require": "./dist/nouveau-flux.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "vite build --mode lib && tsc --emitDeclarationOnly --declaration --outDir dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "leva": "^0.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "leva": ">=0.9.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^5.1.2",
    "typescript": "^5.0.2",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.5.4"
  }
}
