{
  "name": "nouveau-flux",
  "private": true,
  "version": "0.1.0",
  "description": "Animated Art Nouveau SVG frame with psychedelic drift effect",
  "author": "pythagorakase",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pythagorakase/nouveau-flux.git"
  },
  "keywords": [
    "art-nouveau",
    "animation",
    "svg",
    "canvas",
    "psychedelic",
    "react"
  ],
  "type": "module",
  "main": "./dist/nouveau-flux.umd.cjs",
  "module": "./dist/nouveau-flux.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/nouveau-flux.js",
      "require": "./dist/nouveau-flux.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "vite build --mode lib && tsc --emitDeclarationOnly --declaration --outDir dist --noEmit false",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.556.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "leva": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "leva": "^0.9.35",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.0.2",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.5.4"
  }
}
